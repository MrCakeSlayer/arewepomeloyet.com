generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Pomelo {
  hash           String   @id
  date           DateTime
  nitro          Boolean
  possiblyNitro  Boolean  @default(false) @map("possibly_nitro")
  earlySupporter Boolean  @default(false) @map("early_supporter")
  timestamp      DateTime @default(now())

  @@index([date])
  @@index([nitro])
  @@index([possiblyNitro])
  @@index([earlySupporter])
  @@index([timestamp])
  @@map("pomelos")
}
